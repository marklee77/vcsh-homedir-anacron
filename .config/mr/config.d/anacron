[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/anacron.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-anacron.git anacron
setpush  = vcsh anacron remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-anacron.git
           vcsh anacron remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-anacron.git
           vcsh anacron remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-anacron.git

